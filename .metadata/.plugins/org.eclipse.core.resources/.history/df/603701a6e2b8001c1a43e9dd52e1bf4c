import java.util.Comparator;

public class Bruch2 implements Comparator<Bruch2>{

	@Override
	public int compare(Bruch bruch1, Bruch bruch2) {
		float value1 = Math.abs(this.zaehler - this.nenner);
		float value2 = Math.abs(bruch.zaehler - bruch.nenner);
		if(value1 < value2) {
			return -1;
		} 
		else if(value1 > value2) {
			return 1;
		}
		return 0;
	}
}
